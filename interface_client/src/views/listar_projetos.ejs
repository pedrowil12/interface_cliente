<%# A linha abaixo carrega no template o conteúdo total do arquivo header.ejs%>
<%-include('base_header') -%>



<div class="row">
    <% for(var i=0; i< lista_projetos.length; ++i) { %>  
        <div class="card blue" onCLick="window.open('<%=lista_projetos[i].github %>','_blank');">
            <h2>Nome do Projeto: <%=lista_projetos[i].titulo %></h2><br/>
            <span class="descritor">Tipo: <%=lista_projetos[i].tipo %></span><br/>
            <span class="descritor">Tecnologia: <%=lista_projetos[i].tecnologia %></span><br/>
            <span class="descritor">Início:</span> <%=lista_projetos[i].inicio %> <br/>
            <span class="descritor">Término:</span> <%=lista_projetos[i].fim %>            
        </div>
        <% } %>
      </div>

   
<%# A linha abaixo carrega no template o conteúdo total do arquivo footer.ejs %>
<%- include('base_footer') -%>